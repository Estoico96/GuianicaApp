<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Application theme lateral margins. -->
    <style name="LayoutLateralMargin" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:paddingLeft">@dimen/lateral_padding_left</item>
        <item name="android:paddingRight">@dimen/lateral_padding_right</item>

    </style>
</resources>
